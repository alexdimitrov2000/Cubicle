<h1>Browser</h1>
<form action="/" method="GET">
  <input type="text" class="search" name="search" placeholder="Search..." value="{{search}}">
  <input type="number" min="1" name="from" class="difficulty" placeholder="Difficulty from..." value="{{from}}">
  <span>-</span>
  <input type="number" min="1" name="to" class="difficulty" placeholder="Difficulty to..." value="{{to}}">
  <input type="submit" value="search">
</form>
{{#each cubes}}
<div class="cube">
  <img class="cube" src="{{this.imageUrl}}">
  <p class="name">{{this.name}}</p>
  <p><span>Difficulty level:</span> {{this.difficultyLevel}}</p>
  <a class="btn" href="/details/{{this.id}}">Details</a>
  <a class="btn" href="/attach/accessory/{{this.id}}">Attach</a>
</div>
{{else}}
<h2>NO CUBES WERE FOUND</h2>
{{/each}}